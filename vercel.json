{
  "rewrites": [
    {
      "source": "/o-segredo-do-detalhamento",
      "destination": "/index.html"
    },
    {
      "source": "/check-lavagem-segura",
      "destination": "/check-lavagem-segura.html"
    },
    {
      "source": "/politica-privacidade",
      "destination": "/politica-privacidade.html"
    },
    {
      "source": "/termos-uso",
      "destination": "/termos-uso.html"
    },
    {
      "source": "/obrigado",
      "destination": "/obrigado.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
